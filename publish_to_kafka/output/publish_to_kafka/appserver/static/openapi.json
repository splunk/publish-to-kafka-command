{
    "openapi": "3.0.0",
    "info": {
        "title": "publish_to_kafka",
        "version": "0.0.0Rd9afa18",
        "description": "Publish to Kafka for Splunk",
        "contact": {
            "name": ""
        }
    },
    "servers": [
        {
            "url": "https://{domain}:{port}/servicesNS/-/publish_to_kafka",
            "variables": {
                "domain": {
                    "default": "localhost"
                },
                "port": {
                    "default": "8089"
                }
            },
            "description": "Access via management interface"
        }
    ],
    "components": {
        "schemas": {
            "environment": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "bootstrap_servers": {
                        "type": "string"
                    },
                    "security_protocol": {
                        "type": "string",
                        "enum": [
                            "PLAINTEXT",
                            "SASL_PLAINTEXT"
                        ]
                    },
                    "sasl_plain_username": {
                        "type": "string"
                    },
                    "sasl_plain_password": {
                        "type": "string",
                        "format": "password"
                    }
                }
            },
            "environment_without_name": {
                "type": "object",
                "properties": {
                    "bootstrap_servers": {
                        "type": "string"
                    },
                    "security_protocol": {
                        "type": "string",
                        "enum": [
                            "PLAINTEXT",
                            "SASL_PLAINTEXT"
                        ]
                    },
                    "sasl_plain_username": {
                        "type": "string"
                    },
                    "sasl_plain_password": {
                        "type": "string",
                        "format": "password"
                    }
                }
            },
            "logging": {
                "type": "object",
                "properties": {
                    "loglevel": {
                        "type": "string",
                        "enum": [
                            "DEBUG",
                            "INFO",
                            "WARN",
                            "ERROR",
                            "CRITICAL"
                        ]
                    }
                }
            },
            "logging_without_name": {
                "type": "object",
                "properties": {
                    "loglevel": {
                        "type": "string",
                        "enum": [
                            "DEBUG",
                            "INFO",
                            "WARN",
                            "ERROR",
                            "CRITICAL"
                        ]
                    }
                }
            }
        },
        "securitySchemes": {
            "BasicAuth": {
                "type": "http",
                "scheme": "basic"
            }
        }
    },
    "paths": {
        "/publish_to_kafka_environment": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Get list of items for environment",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/environment_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Get list of items for environment",
                "deprecated": false
            },
            "post": {
                "responses": {
                    "200": {
                        "description": "Create item in environment",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/environment_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Create item in environment",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/environment"
                            }
                        }
                    },
                    "required": false
                },
                "deprecated": false
            },
            "parameters": [
                {
                    "name": "output_mode",
                    "in": "query",
                    "required": true,
                    "description": "Output mode",
                    "schema": {
                        "type": "string",
                        "enum": [
                            "json"
                        ],
                        "default": "json"
                    }
                }
            ]
        },
        "/publish_to_kafka_environment/{name}": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Get environment item details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/environment_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Get environment item details",
                "deprecated": false
            },
            "post": {
                "responses": {
                    "200": {
                        "description": "Update environment item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/environment_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Update environment item",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/environment_without_name"
                            }
                        }
                    },
                    "required": false
                },
                "deprecated": false
            },
            "delete": {
                "responses": {
                    "200": {
                        "description": "Delete environment item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/environment_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Delete environment item",
                "deprecated": false
            },
            "parameters": [
                {
                    "name": "name",
                    "in": "path",
                    "required": true,
                    "description": "The name of the item to operate on",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "output_mode",
                    "in": "query",
                    "required": true,
                    "description": "Output mode",
                    "schema": {
                        "type": "string",
                        "enum": [
                            "json"
                        ],
                        "default": "json"
                    }
                }
            ]
        },
        "/publish_to_kafka_settings/logging": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Get list of items for logging",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/logging_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Get list of items for logging",
                "deprecated": false
            },
            "post": {
                "responses": {
                    "200": {
                        "description": "Create item in logging",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "entry": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "content": {
                                                        "$ref": "#/components/schemas/logging_without_name"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Create item in logging",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/logging"
                            }
                        }
                    },
                    "required": false
                },
                "deprecated": false
            },
            "parameters": [
                {
                    "name": "output_mode",
                    "in": "query",
                    "required": true,
                    "description": "Output mode",
                    "schema": {
                        "type": "string",
                        "enum": [
                            "json"
                        ],
                        "default": "json"
                    }
                }
            ]
        }
    },
    "security": [
        {
            "BasicAuth": []
        }
    ]
}